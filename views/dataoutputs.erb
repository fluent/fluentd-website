               ['Splunk', 'splunk.jpg', 'http://docs.fluentd.org/v0.12/articles/out_splunk'],
               [],
               [],
               [],
               [],
               [],
             ].each_slice(7) do |a|
          %>
          <tr>
            <% a.each { |e| %>
            <td style="text-align:center;width:14%;">
              <% unless e.empty? %>
              <a href="<%= e[2] %>">
                <img src="/images/datasources/<%= e[1] %>" height="<%= e.size > 3 ? e[3] : 50 %>px" style="margin:5px;"/>
                <h5><%= e[0] %></h5>
              </a>
              <% end %>
            </td>
            <% } %>
          </tr>
          <% end %>
        </table>
