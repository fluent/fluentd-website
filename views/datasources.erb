<!--=== Breadcrumbs ===-->
<div class="breadcrumbs margin-bottom-40">
  <div class="container">
    <h1 class="pull-left">List of Data Sources</h1>
    <ul class="pull-right breadcrumb">
      <li><a href="/">Home</a></li>
      <li class="active">List of Data Sources</li>
    </ul>
  </div>
</div><!--/breadcrumbs-->
<!--=== End Breadcrumbs ===-->

<!--=== Content Part ===-->
<div class="container">     
  <div class="row">
    <div class="col-md-9">
      <div><h4>Application Logs</h4></div>
      <div class="clearfix">
        <table style="width:100%;">
          <%
             [
               ['Java', 'java.jpg', 'http://docs.fluentd.org/articles/java'],
               ['Ruby', 'ruby.png', 'http://docs.fluentd.org/articles/ruby'],
               ['Ruby on Rails', 'rails.png', '/datasources/rails'],
               ['Python', 'python.png', 'http://docs.fluentd.org/articles/python'],
               ['PHP', 'php.png', 'http://docs.fluentd.org/articles/php'],
               ['Perl', 'perl.png', 'http://docs.fluentd.org/articles/perl'],
               ['Node.js', 'nodejs.png', 'http://docs.fluentd.org/articles/nodejs'],
               ['Scala', 'scala.png', 'http://docs.fluentd.org/articles/scala'],
               ['.NET', 'dotnet.png', 'https://github.com/fluent/NLog.Targets.Fluentd'],
             ].each_slice(7) do |a|
          %>
          <tr>
            <% a.each { |e| %>
            <td style="text-align:center;width:14%;">
              <% unless e.empty? %>
              <a href="<%= e[2] %>">
                <img src="/images/datasources/<%= e[1] %>" height="50px" style="margin:5px;"/>
                <h5><%= e[0] %></h5>
              </a>
              <% end %>
            </td>
            <% } %>
          </tr>
          <% end %>
        </table>
      </div>
      <br/>

      <div><h4>Network Protocols</h4></div>
      <div class="clearfix">
        <table style="width:100%;">
          <%
             [
               ['Syslog', 'syslog.jpg', 'http://docs.fluentd.org/articles/in_syslog'],
               ['HTTP', 'http.png', 'http://docs.fluentd.org/articles/in_http'],
               ['TCP', 'tcp.gif', 'http://docs.fluentd.org/articles/in_forward'],
               ['UDP', 'udp.jpg', 'http://docs.fluentd.org/articles/in_udp'],
               ['Unix Domain Socket', 'unix.gif', 'http://docs.fluentd.org/articles/in_uds'],
               ['SNMP', 'snmp.png', 'https://github.com/iij/fluent-plugin-snmp'],
               ['Scribe Protocol', 'scribe.png', '/datasources/scribe'],
               ['MQTT', 'mqtt.png', 'https://github.com/yuuna/fluent-plugin-mqtt'],
             ].each_slice(7) do |a|
          %>
          <tr>
            <% a.each { |e| %>
            <td style="text-align:center;width:14%;">
              <% unless e.empty? %>
              <a href="<%= e[2] %>" target="_blank">
                <img src="/images/datasources/<%= e[1] %>" height="50px" style="margin:5px;"/>
                <h5><%= e[0] %></h5>
              </a>
              <% end %>
            </td>
            <% } %>
          </tr>
          <% end %>
        </table>
      </div>
      <br/>

      <div><h4>IoT Devices</h4></div>
      <div class="clearfix">
        <table style="width:100%;">
          <%
             [
               ['RasperryPi', 'raspi.png', 'http://docs.fluentd.org/articles/raspberrypi-cloud-data-logger'],
               [],
               [],
               [],
               [],
               [],
               [],
             ].each_slice(7) do |a|
          %>
          <tr>
            <% a.each { |e| %>
            <td style="text-align:center;width:14%;">
              <% unless e.empty? %>
              <a href="<%= e[2] %>" target="_blank">
                <img src="/images/datasources/<%= e[1] %>" height="50px" style="margin:5px;"/>
                <h5><%= e[0] %></h5>
              </a>
              <% end %>
            </td>
            <% } %>
          </tr>
          <% end %>
        </table>
      </div>
      <br/>

      <div><h4>Others</h4></div>
      <div class="clearfix">
        <table style="width:100%;">
          <%
             [
               ['Docker', 'docker.png', '/guides/recipes/docker-logging'],
               ['Twitter', 'twitter.png', '/datasources/twitter'],
               ['MySQL Slow Query Log', 'mysql.png', '/datasources/mysqlslowquerylog'],
               ['PostgreSQL Slow Query Log', 'postgresql.png', 'https://github.com/choplin/fluent-plugin-postgresql'],
               ['Gluster FS', 'gluster.jpg', '/datasources/glusterfs-log'],
               ['Kafka', 'kafka.png', 'https://github.com/htgc/fluent-plugin-kafka'],
               ['Kestrel', 'kestrel.png', '/datasources/kestrel'],
               ['Munin', 'munin.png', 'https://github.com/y-ken/fluent-plugin-munin'],
               ['DStat', 'dstat.png', '/datasources/dstat'],
               ['Windows Event Log', 'windows.jpg', 'http://docs.fluentd.org/articles/windows'],
               ['Netflow', 'netflow.gif', 'https://github.com/repeatedly/fluent-plugin-netflow'],
               ['Postfix', 'postfix.gif', 'http://www.tipstuff.org/2014/01/Postfix-log-centralize-and-analysis-in-realtime-with-fluentd-elasticsearch-and-kibana.html'],
               ['AWS Cloud Watch', 'cloudwatch.png', 'https://github.com/yunomu/fluent-plugin-cloudwatch'],
               ['AWS ELB Log', 'elb.png', 'https://github.com/shinsaka/fluent-plugin-elb-log'],
               ['AWS SQS', 'sqs.png', 'https://github.com/ixixi/fluent-plugin-sqs'],
             ].each_slice(7) do |a|
          %>
          <tr>
            <% a.each { |e| %>
            <td style="text-align:center;width:14%;">
              <% unless e.empty? %>
              <a href="<%= e[2] %>" target="_blank">
                <img src="/images/datasources/<%= e[1] %>" height="50px" style="margin:5px;"/>
                <h5><%= e[0] %></h5>
              </a>
              <% end %>
            </td>
            <% } %>
          </tr>
          <% end %>
        </table>
      </div>
      <br/>
      <br/>
      <div class="row margin-bottom-40">
        <center>
          <a class="btn btn-primary hover-effect" style="font-size: 20px;" href="/plugins/all">Check List of All Plugins</a>
        </center>
      </div>
    </div><!--/col-md-9-->
    <br/>

    <div class="col-md-3">
      <%= erb(:_right_menu, :layout => false) %>
    </div><!--/col-md-3-->
  </div><!--/row-->        

</div><!--/container-->     
<!--=== End Content Part ===-->
